info:
  title: API example
  description: The API description
  version: '1.0'
  contact: {}
tags: []
servers: []
components:
  securitySchemes:
    cookie:
      type: apiKey
      in: cookie
      name: connect.sid
  schemas:
    LoginRequestDTO:
      type: object
      properties:
        email:
          type: string
          format: email
        password:
          type: string
          minLength: 4
          maxLength: 100
      required:
        - email
        - password
    SendOTPRequestDTO:
      type: object
      properties:
        email:
          type: string
          format: email
        type:
          type: string
          enum:
            - REGISTER
            - FORGOT_PASSWORD
      required:
        - email
        - type
    ForgotPasswordRequestDTO:
      type: object
      properties:
        email:
          type: string
          format: email
        code:
          type: string
        newPassword:
          type: string
          minLength: 4
          maxLength: 100
        confirmNewPassword:
          type: string
          minLength: 4
          maxLength: 100
      required:
        - email
        - code
        - newPassword
        - confirmNewPassword
    UpdateProfileRequestDTO:
      type: object
      properties:
        fullName:
          type: string
          minLength: 2
          maxLength: 100
        phone:
          type: string
          pattern: ^(\+84|0)[0-9]{9}$
        avatarUrl:
          type: string
          format: uri
        facebookUrl:
          type: string
          format: uri
        bio:
          type: string
          maxLength: 500
        birthDate:
          type: string
          format: date-time
        gender:
          type: string
          enum:
            - MALE
            - FEMALE
            - OTHER
        userId:
          type: string
          format: uuid
      required:
        - userId
    UpdateDriverLicenseRequestDTO:
      type: object
      properties:
        userId:
          type: string
          format: uuid
        licenseNumber:
          type: string
          minLength: 1
        fullName:
          type: string
          minLength: 2
          maxLength: 100
        dateOfBirth:
          type: string
          format: date-time
        licenseClass:
          type: string
          enum:
            - A1
            - A2
            - B1
            - B2
            - C
            - D
            - E
            - F
        issueDate:
          type: string
          format: date-time
        expiryDate:
          type: string
          format: date-time
        issuePlace:
          type: string
          minLength: 1
        frontImageUrl:
          type: string
          format: uri
        backImageUrl:
          type: string
          format: uri
        selfieImageUrl:
          type: string
          format: uri
      required:
        - userId
    VerifyDriverLicenseRequestDTO:
      type: object
      properties:
        userId:
          type: string
          format: uuid
        isVerified:
          type: boolean
          default: false
        rejectedReason:
          type: string
      required:
        - userId
    UpdateBankAccountRequestDTO:
      type: object
      properties:
        userId:
          type: string
          format: uuid
        bankName:
          type: string
          minLength: 1
        bankCode:
          type: string
          minLength: 1
        accountNumber:
          type: string
          minLength: 1
        accountHolder:
          type: string
          minLength: 2
          maxLength: 100
      required:
        - userId
    UpdateAddressRequestDTO:
      type: object
      properties:
        userId:
          type: string
          format: uuid
        label:
          type: string
          minLength: 1
        city:
          type: string
          minLength: 1
        ward:
          type: string
          minLength: 1
        detail:
          type: string
          minLength: 1
        latitude:
          type: number
        longitude:
          type: number
        id:
          type: string
          format: uuid
      required:
        - userId
        - id
    CreateNotificationRequestDTO:
      type: object
      properties:
        userId:
          type: string
          format: uuid
        type:
          type: string
          enum:
            - WELCOME
            - BOOKING_CREATED
            - BOOKING_CONFIRMED
            - BOOKING_CANCELLED
            - PAYMENT_SUCCESS
            - PAYMENT_FAILED
            - CHECK_IN_REMINDER
            - CHECK_OUT_REMINDER
            - REFUND_PROCESSED
            - REVIEW_REQUEST
            - PROMO_AVAILABLE
            - SYSTEM_ANNOUNCEMENT
            - COMPLAINT_UPDATE
        title:
          type: string
        content:
          type: string
      required:
        - userId
        - type
        - title
        - content
    CreateChatRequestDTO:
      type: object
      properties:
        fromUserId:
          type: string
          format: uuid
        toUserId:
          type: string
          format: uuid
        content:
          type: string
      required:
        - fromUserId
        - toUserId
        - content
    CreateBlogRequestDTO:
      type: object
      properties:
        title:
          type: string
        content:
          type: string
        type:
          type: string
        region:
          type: string
        thumbnail:
          type: string
        tag:
          type: string
        summary:
          type: string
      required:
        - title
        - content
        - type
        - region
        - thumbnail
        - tag
        - summary
